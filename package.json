{
  "name": "mina-aes",
  "version": "1.0.0",
  "description": "A provable AES implementation for the Mina Protocol",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "ts-node src/index.ts",
    "test": "echo 'No tests yet!'",
    "prepare": "husky install",
    "lint": "npx eslint . --max-warnings=0 --fix",
    "format": "npx prettier --write \"**/*.{ts,js}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scaraven/mina-aes.git"
  },
  "author": "scaraven",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scaraven/mina-aes/issues"
  },
  "homepage": "https://github.com/scaraven/mina-aes#readme",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "prettier": "^3.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier -c",
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "o1js": "^2.2.0"
