{
  "name": "mina-aes",
  "version": "1.0.0",
  "description": "A provable AES implementation for the Mina Protocol",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node build/src/index.js",
    "dev": "ts-node src/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky install",
    "lint": "npx eslint src --max-warnings=0 --fix",
    "format": "npx prettier --write \"**/*.ts\"",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scaraven/mina-aes.git"
  },
  "author": "scaraven",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scaraven/mina-aes/issues"
  },
  "homepage": "https://github.com/scaraven/mina-aes#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier -c",
      "eslint src --max-warnings=0"
    ]
  },
  "peerDependencies": {
    "o1js": "^2.*"
  }
}
